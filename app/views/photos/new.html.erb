<h1>Photos</h1>

    <%= form_for(@post) do |f| %>

   <div class="field">
        <%= f.label :caption %><br>
        <%= f.date_field :caption %>
      </div>

      <div class="field">
        <%= f.label :image %><br>
        <%= f.file_field :image %>
      </div>

      <div class="actions">
        <%= f.submit %> 
      </div> 
    <% end %>
 
    
    